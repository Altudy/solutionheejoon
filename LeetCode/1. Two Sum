class Solution {
public:
    vector<int> twoSum(vector<int>& nums, int target) {
        map<int,int> tree;

        for(int i=0; i<nums.size(); i++)
        {
            if(tree.find(target-nums[i])!=tree.end()){
                return vector<int>{tree.find(target-nums[i])->second, i};
            }
            tree.insert(pair<int, int>(nums[i],i));
        }
        return vector<int>{0,1};
    }
};
